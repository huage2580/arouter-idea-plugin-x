<idea-plugin>
    <id>arouter-roadsign</id>
    <name>ARouter Helper X</name>
    <version>2.0.1</version>
    <vendor email="2838666797@qq.com" url="https://github.com/huage2580">Huage2580</vendor>
    <idea-version since-build="201.6668.113"/>
    <depends>com.intellij.modules.java</depends>
    <depends optional="true" config-file="">org.jetbrains.kotlin</depends>
    <description><![CDATA[
    Support to track navigation target.
    ]]></description>

    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider language="JAVA"
                                        implementationClass="com.alibaba.android.arouter.idea.extensions.NavigationLineMarker"/>
        <codeInsight.lineMarkerProvider language="kotlin"
                                        implementationClass="com.alibaba.android.arouter.idea.extensions.KtNavigationLineMarker"/>
        <codeInsight.lineMarkerProvider language="kotlin"
                                        implementationClass="com.alibaba.android.arouter.idea.extensions.KtAnnotationLineMarker"/>
        <codeInsight.lineMarkerProvider language="JAVA"
                                        implementationClass="com.alibaba.android.arouter.idea.extensions.AnnotationLineMarker"/>
    </extensions>

</idea-plugin>